<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <artifactId>esup-blank</artifactId>
        <groupId>org.esupportail.blank</groupId>
        <version>0.2.4</version>
    </parent>
    <groupId>org.esupportail.blank</groupId>
    <artifactId>esup-blank-web-jsf-mobile</artifactId>
    <packaging>war</packaging>
    <description>vue mobile</description>
    <repositories>
        <repository>
            <id>prime-repo</id>
            <name>Prime Technology Maven Repository</name>
            <url>http://repository.prime.com.tr</url>
            <layout>default</layout>
        </repository>
    </repositories>
    <build>
        <defaultGoal>install</defaultGoal>
        <resources>
            <resource>
                <directory>src/main/resources</directory>
                <filtering>true</filtering>
            </resource>
        </resources>
    </build>
    <dependencies>
        <dependency>
            <groupId>javax.servlet</groupId>
            <artifactId>servlet-api</artifactId>
            <version>${javax.servlet.version}</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.primefaces</groupId>
            <artifactId>primefaces</artifactId>
            <version>2.2</version>
        </dependency>
        <dependency>
            <groupId>org.esupportail</groupId>
            <artifactId>esup-commons2-jsf2-exception</artifactId>
            <version>${esupcommons.version}</version>
        </dependency>
        <dependency>
            <groupId>org.hibernate</groupId>
            <artifactId>hibernate-validator</artifactId>
            <version>4.0.2.GA</version>
        </dependency>
        <dependency>
            <groupId>org.esupportail.blank</groupId>
            <artifactId>esup-blank-domain-services</artifactId>
            <version>${project.version}</version>
        </dependency>
    </dependencies>
    <properties>
        <javax.servlet.version>2.5</javax.servlet.version>
        <cas.url>https://sso-cas.univ-rennes1.fr</cas.url>
        <tomcat.host>localhost</tomcat.host>
        <tomcat.port.string>:8080</tomcat.port.string>
        <log.level>INFO</log.level>
        <log.output>stdout</log.output>
        <log.file>/tmp/esup-blank.log</log.file>
        <cache.path>/tmp/esup-blank/cache</cache.path>
    </properties>
</project>
