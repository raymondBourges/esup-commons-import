<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<shortcuts>

  <programGroup defaultName="Example" location="applications"/>

  <shortcut
    name="Example Apache Tomcat"
    target="$INSTALL_PATH/apache-tomcat-6.0.29/bin/startup.sh"
    description="Gestion du Serveur Applicatif"
    programGroup="yes"
    desktop="no"
    applications="no"
    startMenu="no"
    startup="no"     
    type="Application"
    encoding="UTF-8"
    terminal="true"
    KdeSubstUID="false"
