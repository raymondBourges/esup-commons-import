<html>
<head>
<!-- TemplateBeginEditable name="doctitle" -->
<title>ESUP-Portail Example Application</title>
<!-- TemplateEndEditable --> 
<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1"/>
<meta name="keywords" content="esup-example esup-portail commons jsf spring hibernate free open source application web">
<link rel="stylesheet" type="text/css" href="../style.css"/>
<!-- TemplateBeginEditable name="navigation" -->

<script language="JavaScript1.2">
navigationItem = '';
sidebarItem = '';
</script>

<!-- TemplateEndEditable -->

<script language="JavaScript1.2">

var menu_list = new Array("main");

function hide_menus(menu_to_display) {
	for (i = 0; i < menu_list.length ; i++) {
		if (menu_list[i] ==  menu_to_display) {
		    setStyleById(menu_list[i] + '_menu', 'display', 'block');
		}
	}	
}

function show_current_part(part){
	setStyleById(part + '_button', 'background', 'transparent url(../pictures/button_current2.gif) repeat-x top left');
	setStyleById(part + '_button', 'color', 'Black');
}

function show_current_section(current_section){
        document.getElementById(current_section).className = "current";
}

function setStyleById(id, property, value) {
	var element = document.getElementById(id);
	element.style[property] = value;
}

function init(){
	if (navigationItem != '') hide_menus(navigationItem);
	if (navigationItem != '') show_current_part(navigationItem);
	if (sidebarItem != '') show_current_section(sidebarItem);
}

</script>
</head>
<body>
<table id="header" cellspacing="0">
